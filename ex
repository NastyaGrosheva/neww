def scale(s1, s2, a, h): # s1 - площадь города, s2 - площадь семени, a - количество зерен, h - высота зерна
s1 = s1 * 10000000000
l = s1 * a / s2
print(l * h)

def years(w, b): # w - семян в год, b - кол-во семян
print(b/w)

x = 1
for _ in range(64):
x *= 2
print(x) # кол-во зерен )

m = x # масса в граммах
i = 0
while i < 50:
m += x
m /= 1000 # масса в тоннах

print(m)
s = 502.7 # площадь Нск
sz = 0.25 # площадь одного зерна
scale(s, sz, x, 0.5)
